#include <stdio.h>
#include <string.h>

int main() {
  char *buffer[100];
  char *tokenized[100];

  // Get input from user
  printf("Please enter some text: ", stdout);
  getline(&buffer, 99, stdout);

  // Tokenize the input
  strtok_r(&buffer, " ", &tokenized);

  // Print out tokenized input
  printf("\nTokens:\n", stdout);
  while (tokenized != NULL) {
  }



  return 0;
}
