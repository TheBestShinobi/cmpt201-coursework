#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <string.h>



int main() {
  char *buffer[100];
  char **tokenized[100];
  int val = 99; 

  // Get input from user
  printf("Please enter some text: ", stdout);
  getline(&buffer, val, stdout);

  // Tokenize the input
  strtok_r(&buffer, " ", &tokenized);

  // Print out tokenized input
  printf("\nTokens:\n", stdout);
  while (tokenized != NULL) {
  }

  return 0;
}
