#include <stdio.h>
#include <unistd.h>

int main() {
  // Variables
  int pid;

  // Create child process
  pid = fork();

  // If you're the parent, call exec
  if (pid != 0) {
    execl("/bin/ls", "bin/ls", "a");
  }

  // Error handling
  else if (pid == -1) {
    printf("Whoopies something went wrong.");
  }
  // If you're the child, call exec w/ dif output
  else {
    execl("bin/ls", "bin/ls", "a", "l", "h");
  }

}
